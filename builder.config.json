{
  "command": "npm run dev",
  "serverUrl": "http://localhost:3000",
  "commitMode": "draft-prs",
  "allowedCommands": [
    "npm *",
    "pnpm *",
    "yarn *",
    "git diff *",
    "git log *",
    "git status *",
    "echo *",
    "cd *",
    "find *",
    "ls *",
    "curl *"
  ],
  "repoIndexingConfig": {
    "designSystems": [
      "tailwindcss"
    ]
  }
}
